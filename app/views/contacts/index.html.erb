<h1>All Contacts</h1>

<%= will_paginate %>

 <ul class="contacts">
  <% @contacts.each do |contact| %>
  <%= link_to contact.name, contact %>
  <% if signed_in? %>
  | <%= link_to "delete", contact, :confirm => "Are you sure?", :method => :delete %>
   <% end %>
 </ul>
 
<%= will_paginate %>
  <% end %>
