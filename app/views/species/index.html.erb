<h1>All Species</h1>

<%= will_paginate %>

<ul class="species">
  <% @species.each do |species| %>
    <li>
    <%= image_tag species.photo.url %>
    <%= link_to species.species_name, species %>

<% if signed_in? %>
  | <%= link_to "edit", edit_species_path(species) %>
  | <%= link_to "delete", species, :confirm => "Are you sure?", :method => :delete %>
  <% end %>
</li>
  <% end %>
</ul>

<%= will_paginate %>
